{
  "name": "react-component-library-starter-kit",
  "version": "0.0.0",
  "description": "A starter project to build your own React component library",
  "repository": {
    "type": "git",
    "url": "git://github.com/brentertz/react-component-library-starter-kit.git"
  },
  "author": "Brent Ertz <brent.ertz@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean": "rm -rf ./build/**",
    "build": "NODE_ENV=production npm run clean && npm run lint && webpack --config ./components/webpack.config.js && webpack --config ./demo/webpack.config.js",
    "develop": "npm run clean && npm run lint && webpack-dev-server --config ./demo/webpack.config.js -hot --content-base build",
    "lint": "eslint .",
    "test": "jest"
  },
  "dependencies": {},
  "devDependencies": {
    "babel": "^5.6.14",
    "babel-core": "^5.6.15",
    "babel-eslint": "^3.1.20",
    "babel-loader": "^5.2.2",
    "css-loader": "^0.14.5",
    "eslint": "^0.24.0",
    "eslint-loader": "^0.14.1",
    "eslint-plugin-react": "^2.6.4",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.4",
    "jest-cli": "^0.4.14",
    "react": "^0.13.3",
    "sass-loader": "^1.0.2",
    "style-loader": "^0.12.3",
    "webpack": "^1.10.1",
    "webpack-dev-server": "^1.10.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "scriptPreprocessor": "<rootDir>/__tests__/support/preprocessor.js",
    "setupTestFrameworkScriptFile": "<rootDir>/__tests__/support/setup-test-framework.js",
    "testPathIgnorePatterns": [
      "node_modules",
      "__tests__/support"
    ],
    "unmockedModulePathPatterns": [
      "babel",
      "babel-runtime",
      "<rootDir>/?.*/node_modules/react"
    ]
  }
}
